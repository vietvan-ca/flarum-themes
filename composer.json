{
  "name": "vietvan-ca/flarum-themes",
  "description": "A VietVan themed extension for Flarum",
  "type": "flarum-extension",
  "require": {
    "flarum/core": "^1.0.0"
  },
  "autoload": {
    "psr-4": {
      "VietVan\\FlarumThemes\\": "src/"
    }
  },
  "extra": {
    "flarum-extension": {
      "title": "VietVan Theme",
      "category": "theme",
      "icon": {
        "name": "fas fa-palette",
        "backgroundColor": "#C22126",
        "color": "#ffffff"
      }
    }
  }
}
